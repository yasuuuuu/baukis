!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title= document_title
    = stylesheet_link_tag    'customer', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'customer', 'data-turbolinks-track' => true
    = csrf_meta_tags
  %body
    #wrapper
      = render 'customer/shared/header'
      #container
        = yield
      = render 'shared/footer'
